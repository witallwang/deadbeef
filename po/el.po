#: ../main.c:120
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Χρήση: deadbeef [επιλογές] [--] [αρχείο(-α)]\n"

#: ../main.c:121
#, c-format
msgid "Options:\n"
msgstr "Επιλογές:\n"

#: ../main.c:122
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr " --help ή -h Εμφάνιση κειμένου βοήθειας (αυτό το κείμενο) και έξοδος\n"

#: ../main.c:123
#, c-format
msgid "   --quit             Quit player\n"
msgstr "--quit Έξοδος\n"

#: ../main.c:124
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "--version Εμφάνιση πληροφοριών έκδοσης και έξοδος\n"

#: ../main.c:125
#, c-format
msgid "   --play             Start playback\n"
msgstr "--play Εκκίνηση αναπαραγωγής\n"

#: ../main.c:126
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "--stop Διακοπή αναπαραγωγής\n"

#: ../main.c:127
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "--pause Παύση αναπαραγωγής\n"

#: ../main.c:128
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "--toggle-pause Εναλλαγή παύσης\n"

#: ../main.c:129
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr ""
"--play-pause Εκκίνηση αναπαραγωγής αν είναι σταματημένο, αλλιώς εναλλαγή "
"παύσης\n"

#: ../main.c:130
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "--next Επόμενο κομμάτι στη λίστα αναπαραγωγής\n"

#: ../main.c:131
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "--prev Προηγούμενο κομμάτι στη λίστα αναπαραγωγής\n"

#: ../main.c:132
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "--random Τυχαίο κομμάτι στη λίστα αναπαραγωγής\n"

#: ../main.c:133
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "--queue Προσάρτηση αρχείου(-ων) στην υπάρχουσα λίστα αναπαραγωγής\n"

#: ../main.c:134
#, c-format
msgid ""
"   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr ""
"--gui PLUGIN  Επιλέγει ποιό πρόσθετο γραφικής διεπαφής θα χρησιμοποιηθεί, το "
"προκαθορισμένο είναι το \"GTK2\"\n"

#: ../main.c:135
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "--nowplaying FMT Εμφάνιση μορφοποιημένου ονόματος κομματιού\n"

#: ../main.c:136
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr ""
"FMT %%-συντακτικό: [a]rtist, [t]itle, al[b]um,\n"
" [l]ength, track[n]umber, [y]ear, [c]omment,\n"
" copy[r]ight, [e]lapsed\n"

#: ../main.c:139
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""

#: ../main.c:140
#, c-format
msgid "                      for more info, see %s\n"
msgstr "για περισσότερες πληροφορίες, δες %s\n"

#: ../main.c:141
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr ""

#: ../main.c:142
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""

#: ../main.c:143
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-Player/deadbeef/"
"wiki/Title-formatting-2.0\n"
msgstr ""

#: ../main.c:144
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""

#: ../main.c:145
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr ""

#: ../main.c:146
