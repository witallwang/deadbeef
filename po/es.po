#: ../main.c:120
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Modo de empleo: deadbeef [opciones] [--] [archivo(s)]\n"

#: ../main.c:121
#, c-format
msgid "Options:\n"
msgstr "Opciones:\n"

#: ../main.c:122
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  o  -h      Mostrar la ayuda (este mensaje) y salir\n"

#: ../main.c:123
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit             Salir del reproductor\n"

#: ../main.c:124
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr "   --version          Mostrar la versión y salir\n"

#: ../main.c:125
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play             Iniciar reproducción\n"

#: ../main.c:126
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop             Parar reproducción\n"

#: ../main.c:127
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause            Pausar reproducción\n"

#: ../main.c:128
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Alternar pausa\n"

#: ../main.c:129
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr ""
"   --play-pause       Iniciar la reproducción si está detenida, pausar en "
"caso contrario\n"

#: ../main.c:130
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next             Siguiente pista de la lista de reproducción\n"

#: ../main.c:131
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev             Pista anterior de la lista de reproducción \n"

#: ../main.c:132
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random           Canción aleatoria de la lista de reproducción\n"

#: ../main.c:133
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue            Añadir archivo(s) a la lista de reproducción\n"

#: ../main.c:134
#, c-format
msgid ""
"   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr ""
"   --gui COMPLEMENTO  Indica cual complemento de IGU usar; \"GTK2\" es el "
"predeterminado\n"

#: ../main.c:135
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr ""
"   --nowplaying FMT   Envía por la salida estándar el nombre formateado de "
"la pista\n"

#: ../main.c:136
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr ""
"                      FMT %%-sintaxis:  a: Artista, t: Título, b: Álbum,\n"
"                      l: Duración, n: Pista, y: Año, c: Comentario,\n"
"                      r: Copyright, e: Tiempo restante\n"

#: ../main.c:139
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""
"                      ejemplo: --nowplaying \"%%a - %%t\" mostraría "
"\"artista - título\"\n"

#: ../main.c:140
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      más información en %s\n"

#: ../main.c:141
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      NOTA:  --nowplaying está considerada obsoleta.\n"

#: ../main.c:142
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""
"   --nowplaying-tf FMT  Envía por la salida estándar el nombre formateado de "
"la pista,\n"
"                      usando el nuevo formato de título\n"

#: ../main.c:143
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-Player/deadbeef/"
"wiki/Title-formatting-2.0\n"
msgstr ""
"                      Sintaxis FMT: http://github.com/DeaDBeeF-Player/"
"deadbeef/wiki/Title-formatting-2.0\n"

#: ../main.c:144
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""
"                      ejemplo: --nowplaying-tf \"%%artist%% - %%title%%\" "
"mostraría \"artista - título\"\n"

#: ../main.c:145
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "--volume NUM       Establece o muestra el volumen.\n"

#: ../main.c:146
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents (if no "
"suffix) or dB [-50, 0].\n"
msgstr ""
"NUM puede ser especificado como porcentaje (si no hay sufijo) o como dB "
"[-50, 0]\n"

#: ../main.c:147
#, c-format
msgid "                      Examples: --volume 80 or --volume -20dB\n"
msgstr "ejemplos: --volume 80 ó --volume -20dB\n"

