#: ../main.c:120
#, c-format
msgid "Usage: deadbeef [options] [--] [file(s)]\n"
msgstr "Выкарыстанне: deadbeef [options] [--] [file(s)]\n"

#: ../main.c:121
#, c-format
msgid "Options:\n"
msgstr "Налады:\n"

#: ../main.c:122
#, c-format
msgid "   --help  or  -h     Print help (this message) and exit\n"
msgstr "   --help  ці  -h     Адлюстраваць даведку (гэты тэкст) і выйсці\n"

#: ../main.c:123
#, c-format
msgid "   --quit             Quit player\n"
msgstr "   --quit              Выйсці з плэера\n"

#: ../main.c:124
#, c-format
msgid "   --version          Print version info and exit\n"
msgstr ""
"   --version           Адлюстраваць інфармацыю аб версіі праграмы і выйсці\n"

#: ../main.c:125
#, c-format
msgid "   --play             Start playback\n"
msgstr "   --play              Пачаць граць\n"

#: ../main.c:126
#, c-format
msgid "   --stop             Stop playback\n"
msgstr "   --stop              Спыніць прайграванне\n"

#: ../main.c:127
#, c-format
msgid "   --pause            Pause playback\n"
msgstr "   --pause             Прыпыніць прайграванне\n"

#: ../main.c:128
#, c-format
msgid "   --toggle-pause     Toggle pause\n"
msgstr "   --toggle-pause     Пераключыць паўзу\n"

#: ../main.c:129
#, c-format
msgid ""
"   --play-pause       Start playback if stopped, toggle pause otherwise\n"
msgstr ""
"   --play-pause       Пачаць прайграванне, калі спынена, іначай пераключыць "
"паўзу\n"

#: ../main.c:130
#, c-format
msgid "   --next             Next song in playlist\n"
msgstr "   --next              Настуная песня ў плэйлісце\n"

#: ../main.c:131
#, c-format
msgid "   --prev             Previous song in playlist\n"
msgstr "   --prev              Папярэдняя песня ў плэйлісце\n"

#: ../main.c:132
#, c-format
msgid "   --random           Random song in playlist\n"
msgstr "   --random            Выпадковая песня ў плэйлісце\n"

#: ../main.c:133
#, c-format
msgid "   --queue            Append file(s) to existing playlist\n"
msgstr "   --queue             Дадаць файл(ы) у існуючы плэйліст\n"

#: ../main.c:134
#, c-format
msgid ""
"   --gui PLUGIN       Tells which GUI plugin to use, default is \"GTK2\"\n"
msgstr ""
"   --gui PLUGIN       Выбраць убудову інтэрфейсу, перадвызначана \"GTK2\"\n"

#: ../main.c:135
#, c-format
msgid "   --nowplaying FMT   Print formatted track name to stdout\n"
msgstr "   --nowplaying FMT    Адлюстраваць адфармаціраваную назву трэка\n"

#: ../main.c:136
#, c-format
msgid ""
"                      FMT %%-syntax: [a]rtist, [t]itle, al[b]um,\n"
"                      [l]ength, track[n]umber, [y]ear, [c]omment,\n"
"                      copy[r]ight, [e]lapsed\n"
msgstr ""
"                       FMT %%-сінтаксіс: %%a-выканаўца, %%t-назва, %%b-"
"альбом,\n"
"                       %%l-даўжыня, %%n-нумар трэка, %%y-год, %%c-"
"каментарый,\n"
"                       %%r-аўтарскія правы, %%e-пройдзены час\n"

#: ../main.c:139
#, c-format
msgid ""
"                      example: --nowplaying \"%%a - %%t\" should print "
"\"artist - title\"\n"
msgstr ""
"                      Напрыклад: --nowplaying \"%%a - %%t\" выводзіць "
"\"выканаўца - назва\"\n"

#: ../main.c:140
#, c-format
msgid "                      for more info, see %s\n"
msgstr "                      для дадатковай інфармацыі глядзіце %s\n"

#: ../main.c:141
#, c-format
msgid "                      NOTE: --nowplaying is deprecated.\n"
msgstr "                      НАТАТКА: параметр --nowplaying састарэў.\n"

#: ../main.c:142
#, c-format
msgid ""
"   --nowplaying-tf FMT  Print formatted track name to stdout, using the new "
"title formatting\n"
msgstr ""
"   --nowplaying-tf FMT    Вывесці на экран адфармаціраваную назву трэка, "
"выкарыстоўваючы новае фармаціраванне загалоўка\n"

#: ../main.c:143
#, c-format
msgid ""
"                      FMT syntax: http://github.com/DeaDBeeF-Player/deadbeef/"
"wiki/Title-formatting-2.0\n"
msgstr ""
"                      Сінтаксіс FMT: http://github.com/DeaDBeeF-Player/"
"deadbeef/wiki/Title-formatting-2.0\n"

#: ../main.c:144
#, c-format
msgid ""
"                      example: --nowplaying-tf \"%%artist%% - %%title%%\" "
"should print \"artist - title\"\n"
msgstr ""
"                      напрыклад: --nowplaying \"%%a - %%t\" выводзіць "
"\"выканаўца - назва\"\n"

#: ../main.c:145
#, c-format
msgid "   --volume [NUM]     Print or set deadbeef volume level.\n"
msgstr "   --volume [ЛІК]     Вывад альбо прызначэнне ўзроўня гуку deadbeef.\n"

#: ../main.c:146
#, c-format
msgid ""
"                      The NUM parameter can be specified in percents (if no "
"suffix) or dB [-50, 0].\n"
msgstr ""
"                      Параметр ЛІК можа быць зададзены ў працэнтах (калі "
"няма суфіксу) альбо дБ [-50, 0].\n"

#: ../main.c:147
#, c-format
msgid "                      Examples: --volume 80 or --volume -20dB\n"
msgstr "                      Прыклады: --volume 80 або --volume -20dB\n"

